<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/homepagebg"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundcolor"
    tools:context=".Homepage">


    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="87dp"
        android:layout_height="61dp"
        android:src="@drawable/__removebg_preview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        android:contentDescription="TODO" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="87dp"
        android:layout_height="61dp"
        android:layout_marginStart="4dp"
        android:src="@drawable/__removebg_preview"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="46dp"
        android:scaleType="centerCrop"
        android:src="@drawable/title2_removebg_2222"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/main_star_third"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="32dp"
        android:src="@drawable/bigstar"
        app:layout_constraintBottom_toTopOf="@+id/letsplaybtn"
        app:layout_constraintStart_toEndOf="@+id/main_star_second"
        app:layout_constraintTop_toTopOf="@+id/main_star_second"
        app:layout_constraintVertical_bias="0.116" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="92dp"
        android:layout_height="86dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/__removebg_preview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="92dp"
        android:layout_height="86dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="44dp"
        android:src="@drawable/__removebg_preview"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="92dp"
        android:layout_height="86dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/__removebg_preview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="92dp"
        android:layout_height="86dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/__removebg_preview"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/main_star_second"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="44dp"
        android:src="@drawable/bigstar"
        app:layout_constraintBottom_toTopOf="@+id/letsplaybtn"
        app:layout_constraintStart_toEndOf="@+id/main_star_one"
        app:layout_constraintTop_toTopOf="@+id/main_star_one"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/main_star_one"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="44dp"
        android:src="@drawable/bigstar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="76dp"
        android:layout_height="64dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="40dp"
        android:rotation="-13"
        android:src="@drawable/rocket22"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="58dp"
        android:layout_height="79dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="92dp"
        android:rotation="-20"
        android:src="@drawable/spaceship222"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/letsplaybtn"
        app:layout_constraintVertical_bias="0.69" />

    <Button
        android:id="@+id/letsplaybtn"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="56dp"
        android:layout_marginTop="30dp"
        android:text="Let's Play"
        android:textColor="#3c0384"
        app:backgroundTint="#ffffff"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/main_star_one" />

</androidx.constraintlayout.widget.ConstraintLayout>